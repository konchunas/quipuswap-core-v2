{
  "name": "quipuswap-core-v2",
  "version": "0.0.1",
  "license": "MIT",
  "description": "Quipuswap Core V2",
  "scripts": {
    "start-sandbox": "docker run --rm --name my-sandbox -e flextesa_node_cors_origin='*' -e block_time=1 --detach -p 8732:20000 tqtezos/flextesa:20210602 granabox start",
    "clean": "rm -rf ./build ./contracts/compiled",
    "pre-compile": "ts-node scripts/cli.ts compile -c tez_store -o contracts/compiled",
    "compile-lambdas": "ts-node scripts/cli.ts compile-lambda -j storage/json/DexCoreFunctions.json -c contracts/main/dex_core.ligo",
    "compile": "yarn pre-compile && ts-node scripts/cli.ts compile -f json",
    "full-compile": "yarn clean && yarn compile && yarn compile-lambdas",
    "migrate": "yarn compile && ts-node scripts/cli.ts migrate",
    "migrate-granadanet": "yarn compile && ts-node scripts/cli.ts migrate -n granadanet",
    "migrate-mainnet": "yarn compile && ts-node scripts/cli.ts migrate -n mainnet",
    "test": "ts-mocha --paths test/*.spec.ts --bail"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/madfish-solutions/quipuswap-core-v2.git"
  },
  "dependencies": {
    "@taquito/michelson-encoder": "^10.1.2",
    "@taquito/rpc": "^10.1.2",
    "@taquito/signer": "^10.1.2",
    "@taquito/taquito": "^10.1.2",
    "@types/expect": "^24.3.0",
    "@types/mocha": "^9.0.0",
    "assert": "^2.0.0",
    "child_process": "^1.0.2",
    "fs": "^0.0.1-security",
    "mocha": "^9.1.2",
    "ts-mocha": "^8.0.0",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3",
    "yargs": "^17.2.1"
  },
  "bugs": {
    "url": "https://github.com/madfish-solutions/quipuswap-core-v2/issues"
  },
  "homepage": "https://github.com/madfish-solutions/quipuswap-core-v2#readme",
  "keywords": [
    "tezos",
    "ligo",
    "blockchain"
  ]
}
